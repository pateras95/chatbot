{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'ChatBos',\n  data() {\n    return {\n      messages: [],\n      currentMessage: null\n    };\n  },\n  methods: {\n    async sendMessage(message) {\n      this.messages.push({\n        from: 'user',\n        data: message\n      });\n      await axios.post('http://localhost:3000/ask-to-chat-gpt', {\n        message: message\n      }).then(response => {\n        this.messages.push(response.data);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","messages","currentMessage","methods","sendMessage","message","push","from","post","then","response"],"sources":["src/App.vue"],"sourcesContent":["<template>\n    <div class=\"hello\">\n        <h1>Ask to chatGpt</h1>\n        <input v-model=\"currentMessage\" type=\"text\"> <span><button @click=\"sendMessage(currentMessage)\">Ask</button></span>\n        <div class=\"messageBox\">\n            <template v-for=\"message in messages\">\n                <div :class=\"message.from=='user' ? 'messageFromUser' :'messageFromChatGpt'\" :key=\"message\" v-html=\"message.data\"></div>\n            </template>\n        </div>\n    </div>\n</template>\n\n<script>\n    import axios from 'axios'\n    export default {\n        name: 'ChatBos',\n        data() {\n            return {\n                messages: [],\n                currentMessage: null\n            }\n        },\n\n        methods: {\n            async sendMessage(message) {\n                this.messages.push({\n                    from: 'user',\n                    data: message\n                })\n                await axios.post('http://localhost:3000/ask-to-chat-gpt', {\n                    message: message\n                })\n                    .then((response) => {\n                        this.messages.push(response.data)\n                    })\n            }\n        }\n    }\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\ninput {\n  width: 300px;\n  padding: 10px;\n}\n\nbutton {\n  height: 40px;\n  background-color: powderblue;\n  padding: 10px;\n}\n.messageBox {\n  height: 500px;\n  background-color: gainsboro;\n  /* margin-left: 10%;\n  margin-right: 10%; */\n  margin: 0 20% 0 20%;\n  margin-top: 20px;\n  padding: 5%;\n}\n\n.messageFromUser {\n  text-align: right;\n  background-color: aliceblue;\n  border-radius: 10px;\n  padding: 10px;\n  margin-top: 15px;\n  margin-bottom: 15px;\n  width: 30%;\n  margin-left: 70%;\n}\n\n.messageFromChatGpt {\n  text-align: left;\n  background-color: antiquewhite;\n  border-radius: 10px;\n  padding: 10px;\n  margin-top: 15px;\n  margin-bottom: 15px;\n  width: 30%;\n  margin-right: 70%;\n}\n</style>\n"],"mappings":";AAaA,OAAAA,KAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;MACAC,cAAA;IACA;EACA;EAEAC,OAAA;IACA,MAAAC,YAAAC,OAAA;MACA,KAAAJ,QAAA,CAAAK,IAAA;QACAC,IAAA;QACAP,IAAA,EAAAK;MACA;MACA,MAAAP,KAAA,CAAAU,IAAA;QACAH,OAAA,EAAAA;MACA,GACAI,IAAA,CAAAC,QAAA;QACA,KAAAT,QAAA,CAAAK,IAAA,CAAAI,QAAA,CAAAV,IAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}